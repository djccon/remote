<h1>Listing command_items</h1>

<table>
  <tr>
    <th>Shot</th>
    <th>Command</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @command_items.each do |command_item| %>
  <tr>
    <td><%= command_item.shot_id %></td>
    <td><%= command_item.command_id %></td>
    <td><%= link_to 'Show', command_item %></td>
    <td><%= link_to 'Edit', edit_command_item_path(command_item) %></td>
    <td><%= link_to 'Destroy', command_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Command item', new_command_item_path %>
